{
  "name": "test1.1",
  "version": "7.8.7",
  "description": "Стандартный шаблон верстки",
  "license": "ISC",
  "keywords": [],
  "main": "index.js",
  "projectFolders": {
    "ignore-detect": [
      "_HTML",
      "www"
    ]
  },
  "nodeSassConfig": {
    "binaryPath": "/system/node-sass/binding.node"
  },
  "scripts": {
    "analyze": "webpack-bundle-analyzer",
    "clear": "gulp clear",
    "start": "webpack && npm run gulp-start",
    "dev": "npm run gulp-start -- -w --bs-webpack",
    "dev-media": "npm run gulp-start-media -- -w --bs-webpack",
    "prod": "npm run gulp-start -- -pw --bs-webpack",
    "critical": "gulp -r \"bs, critical, sass-critical\" -n",
    "test": "gulp -r \"system-lint, ejs-lint, js-lint\"",
    "test-full": "gulp -r \"system-lint, ejs-lint, js-lint, sass-lint\"",
    "build": "npm test -- --no-watch --fix && npm run build-without-tests",
    "svg-sprite": "gulp -r \"svg-sprite\"",
    "svg-sprite-ftp": "gulp -r \"svg-sprite\" --ftp",
    "modernizr": "gulp modernizr",
    "modernizr-ftp": "gulp modernizr --ftp",
    "assets-dev": "gulp -r \"static-assets, favicon-ico, svg-sprite\" -w --bs",
    "assets-prod": "npm run assets-dev -- -p",
    "assets-ftp": "npm run assets-dev -- --ftp --no-watch --no-bs",
    "assets-build": "gulp static-clear && npm run assets-prod -- --no-watch --no-bs",
    "ejs-dev": "gulp -r \"svg-sprite, ejs-assets, ejs-markup\" -w --bs",
    "ejs-prod": "npm run ejs-dev -- -p",
    "ejs-test": "gulp -r \"ejs-lint\" -w --fix",
    "ejs-ftp": "npm run ejs-dev -- --ftp --no-watch --no-bs",
    "ejs-docs": "gulp -r \"ejs-docs, docs-index\" -w",
    "ejs-build": "npm run ejs-test -- --no-watch && gulp ejs-clear && npm run ejs-prod -- --no-watch --no-bs",
    "sass-dev": "gulp -r \"sass-main, sass-critical, sass-assets\" -w --bs",
    "sass-prod": "npm run sass-dev -- -p",
    "sass-test": "gulp -r \"sass-lint\" -w",
    "sass-ftp": "npm run sass-dev -- --ftp --no-watch --no-bs",
    "sass-docs": "gulp -r \"sass-docs, sass-tutorials, docs-index\" -w",
    "sass-build": "npm run sass-test -- --no-watch && gulp sass-clear && npm run sass-prod -- --no-watch --no-bs",
    "js-dev": "gulp -r \"js-critical, js-assets, modernizr\" -w --bs-webpack",
    "js-prod": "npm run js-dev -- -p",
    "js-test": "gulp -r \"js-lint\" -w --fix",
    "js-ftp": "webpack --define ftp && npm run js-dev -- --ftp --no-watch --no-bs-webpack",
    "js-docs": "gulp -r \"js-docs, docs-index\" -w",
    "js-build": "npm run js-test -- --no-watch && gulp js-clear && npm run js-prod -- --no-watch --no-bs-webpack",
    "system-test": "gulp -r \"system-lint\" -w --fix",
    "system-docs": "gulp -r \"system-docs, docs-index\" -w",
    "docs-upload": "gulp docs-upload",
    "clear-no-ask": "gulp clear --define no-ask-before-delete",
    "inkubator": "webpack --define ftp && npm run gulp-start -- --ftp && npm run ftp-upload",
    "inkubator-clear": "gulp clear --ftp",
    "inkubator-upload": "gulp ftp-upload",
    "gulp-start": "gulp -r \"sass-main, sass-critical, modernizr, ejs-markup\" -b \"ejs-assets, sass-assets, js-static, js-critical, static-assets, favicon-ico, svg-sprite\"",
    "gulp-start-media": "gulp -r \"sass-main, sass-critical, svg-sprite, modernizr\" -b \"sass-assets, js-assets, js-critical, static-assets, favicon-ico\" --define no-svg-list",
    "build-without-tests": "npm run clear && webpack -p && npm run gulp-start -- -p --force-run --verbose"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "engines": {
    "node": ">=6.3.1"
  },
  "dependencies": {
    "blazy": "^1.8.2",
    "custom-jquery-methods": "^1.2.2",
    "es6-promise-polyfill": "^1.2.0",
    "inputmask": "^3.3.11",
    "intersection-observer": "^0.4.3",
    "jquery": "^3.2.1",
    "jquery-validation": "^1.17.0",
    "lodash": "^4.17.4",
    "lozad": "^1.0.9",
    "magnific-popup": "^1.1.0",
    "normalize.css": "^7.0.0",
    "noty": "^3.1.3",
    "prismjs": "^1.8.4",
    "slick-carousel": "^1.8.1"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.6",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.6.1",
    "chalk": "^2.3.0",
    "classnames": "^2.2.5",
    "css-loader": "^0.28.7",
    "css-mqpacker": "^6.0.1",
    "cssnano": "^3.10.0",
    "del": "^3.0.0",
    "file-loader": "^1.1.5",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-babel": "^7.0.0",
    "gulp-changed": "^3.1.1",
    "gulp-cheerio": "^0.6.2",
    "gulp-ejs-monster": "^3.1.1",
    "gulp-happiness": "^2.0.2",
    "gulp-happiness-scss": "^5.2.1",
    "gulp-if": "^2.0.2",
    "gulp-imagemin": "^3.4.0",
    "gulp-include": "^2.3.1",
    "gulp-modernizr-wezom": "^1.1.3",
    "gulp-newer": "^1.3.0",
    "gulp-not-supported-file": "^1.2.1",
    "gulp-notify": "^3.0.0",
    "gulp-postcss": "^7.0.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass-monster": "^1.0.3",
    "gulp-sourcemaps": "^2.6.1",
    "gulp-svg-sprite": "^1.3.7",
    "gulp-svgmin": "^1.2.4",
    "gulp-uglify": "^3.0.0",
    "gulp-util": "^3.0.8",
    "gulp-watch-and-touch": "^1.1.2",
    "inquirer": "^3.3.0",
    "js-beautify": "^1.7.4",
    "jsdom": "^11.4.0",
    "marked": "^0.3.6",
    "node-notifier": "^5.1.2",
    "sort-css-media-queries": "^1.3.4",
    "strip-ansi": "^4.0.0",
    "style-loader": "^0.19.0",
    "through2": "^2.0.3",
    "url-loader": "^0.6.2",
    "vinyl": "^2.1.0",
    "webpack": "^3.8.1",
    "webpack-bundle-analyzer": "^2.9.1",
    "yargs": "^9.0.1"
  },
  "optionalDependencies": {
    "browser-sync": "^2.18.13",
    "gulp-jsdoc3": "^1.0.1",
    "jsdoc-simple-theme": "^1.2.6",
    "penthouse": "^1.1.2",
    "sassdoc": "^2.4.0",
    "vinyl-ftp": "^0.6.0",
    "webpack-dev-middleware": "^1.12.1",
    "write-file-webpack-plugin": "^4.2.0"
  }
}
